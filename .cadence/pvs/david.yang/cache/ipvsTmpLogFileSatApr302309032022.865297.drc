OR ESD_NMOS_SOURCE_pre1 ESD_NMOS_SOURCE_pre3 -outputlayer ESD_NMOS_SOURCE_pre4;
NOT ESD_NSD ESD_NMOS_SOURCE_pre4 -outputlayer ESD_NMOS_DRAIN_pre4;
