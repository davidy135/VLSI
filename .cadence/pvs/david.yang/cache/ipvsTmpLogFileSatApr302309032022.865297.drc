    EDGE_BOOLEAN -coincident_only -outside PFD_Collector PFD_STI -outputlayer B;
    EDGE_LENGTH B -lt "ESD_61g_MIN";
    EDGE_LENGTH B -gt "ESD_61g_MAX";
}

