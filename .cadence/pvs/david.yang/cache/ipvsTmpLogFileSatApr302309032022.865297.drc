RULE IND.R.13 {
    CAPTION "INDDMY enclosure of top metal(inductor) >= 4 um.";
    ANGLE INDDMY -eq 45 -outputlayer INDDMYx45;
    EDGE_BOOLEAN -coincident_only -not INDDMY INDDMYx45 -outputlayer INDDMYx090;
