    ENC IND_TM_core_check INDDMYx45 -lt "4 - GRID" -abut -lt 90 -metric opposite -output region;
    ENC IND_TM_core_check INDDMYx090 -lt 4 -abut -lt 90 -metric opposite -output region;
}

