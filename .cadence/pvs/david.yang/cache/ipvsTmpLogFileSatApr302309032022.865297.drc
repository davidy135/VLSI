RULE CSR.R.2:B {
    CAPTION "M1~M8 must be stacked and coincident the edge.";
    XOR CC_M1 CC_M2;
    XOR CC_M2 CC_M3;
    XOR CC_M3 CC_M4;
    XOR CC_M4 CC_M5;
    XOR CC_M5 CC_M6;
    XOR CC_M6 CC_M7;
    XOR CC_M7 CC_M8;
}

