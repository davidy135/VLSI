    DENSITY ODx_MAX_CORE CHIP_MAX_CORE -gt OD_DN_3H_CORE -window "OD_DN_3H_W" -step "OD_DN_3H_S" -inside_of layer CHIPx -backup -expr "  AREA(ODx_MAX_CORE)/AREA(CHIP_MAX_CORE)  " -outputlayer ERR_WIN;
