SELECT -interact HV_TOL_IO_GATE HV_TOL_IO_NMOS_DRAIN -outputlayer HV_TOL_IO_GATEa;
SELECT -interact -not HV_TOL_IO_GATE HV_TOL_IO_NMOS_DRAIN _EPTMPL270213;
